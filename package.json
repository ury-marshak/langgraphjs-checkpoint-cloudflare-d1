{
  "name": "langgraph-checkpoint-cloudflare-d1",
  "version": "0.1.0",
  "description": "LangGraph checkpointer for Cloudflare D1 database",
  "keywords": [
    "langchain",
    "langgraph",
    "cloudflare",
    "d1",
    "checkpoint",
    "database"
  ],
  "homepage": "https://github.com/ury-marshak/langgraphjs-checkpoint-cloudflare-d1",
  "repository": {
    "type": "git",
    "url": "git@github.com:ury-marshak/langgraphjs-checkpoint-cloudflare-d1.git"
  },
  "bugs": {
    "url": "https://github.com/ury-marshak/langgraphjs-checkpoint-cloudflare-d1/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "yarn build:internal",
    "build:internal": "yarn clean && node ./node_modules/@langchain/scripts/dist/build/index.js --create-entrypoints --pre --tree-shaking",
    "clean": "rm -rf dist/ dist-cjs/ .turbo/",
    "test": "cd test && ./test-checkpointer.sh",
    "prepack": "npx yarn build"
  },
  "author": "Ury Marshak",
  "license": "MIT",
  "peerDependencies": {
    "@langchain/core": ">=0.3.0 <0.4.0",
    "@langchain/langgraph-checkpoint": "^0.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250826.0",
    "@langchain/core": "^0.3.72",
    "@langchain/langgraph-checkpoint": "^0.1.0",
    "@langchain/scripts": ">=0.1.3 <0.2.0",
    "@tsconfig/recommended": "^1.0.3",
    "@types/uuid": "^10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv": "^16.3.1",
    "dpdm": "^3.12.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "miniflare": "^4.20250823.0",
    "prettier": "^2.8.3",
    "release-it": "^19.0.2",
    "rollup": "^4.37.0",
    "tsx": "^4.19.3",
    "typescript": "^4.9.5 || ^5.4.5",
    "vitest": "^3.1.2",
    "wrangler": "^4.33.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./index.d.ts",
        "require": "./index.d.cts",
        "default": "./index.d.ts"
      },
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "index.d.cts"
  ],
  "dependencies": {
    "cloudflare": "^4.5.0"
  }
}
